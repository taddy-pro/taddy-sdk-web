{
  "name": "taddy-sdk-web",
  "version": "1.3.14",
  "description": "Taddy Web SDK",
  "keywords": [
    "taddy"
  ],
  "homepage": "https://taddy.gitbook.io/docs/sdk/web",
  "license": "MIT",
  "type": "commonjs",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist/types"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc -p tsconfig.types.json",
    "build:js": "webpack",
    "prepublishOnly": "npm run build:types",
    "deploy": "export $(cat .env | xargs) && npm run build:js && rsync -av --delete ./dist/js/ $DEPLOY_USER@$DEPLOY_SERVER:$DEPLOY_PATH && ./cf-purge.sh"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "css-loader": "^7.1.2",
    "prettier": "^3.5.0",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@types/telegram-web-app": "latest",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "preact": "^10.26.9"
  }
}
